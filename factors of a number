//-----------------------factor of a number----------------------------
#include<iostream>
using namespace std;

int main(){
    int i , n;
    cout<<"enter the value of n"<<endl;
    cin>>n;
    for(int i=1 ; i<=n ;i++){
        if(n%i==0)
        cout<<"the factor of the num is "<<i<<endl;
    }
    return 0;
}
